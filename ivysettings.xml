<ivysettings>
  <settings defaultResolver="chained"/>
  <resolvers>
    <chain name="chained">
        <ibiblio name="maven2.repo" m2compatible="true"/>
        <ibiblio name="java.net2" root="http://download.java.net/maven/2/" m2compatible="true"/>
        <ibiblio name="Scala-Tools Maven2 Repository" root="http://scala-tools.org/repo-releases" m2compatible="true"/>
        <ibiblio name="java.maven2.repo" root="http://download.java.net/maven/2" m2compatible="true"/>
        <ibiblio name="ibiblio"/>
    </chain>
  </resolvers>
  
  <modules>
        <module organisation="com.sun.jersey" resolver="java.maven2.repo"/>
    </modules>
</ivysettings>
